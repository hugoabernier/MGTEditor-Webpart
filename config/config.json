{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "mgt-editor-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mgtEditor/MgtEditorWebPart.js",
          "manifest": "./src/webparts/mgtEditor/MgtEditorWebPart.manifest.json"
        }
      ]
    },
    "mgt-react-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mgtReact/MgtReactWebPart.js",
          "manifest": "./src/webparts/mgtReact/MgtReactWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "MgtEditorWebPartStrings": "lib/webparts/mgtEditor/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "MgtReactWebPartStrings": "lib/webparts/mgtReact/loc/{locale}.js"
  }
}
